bogofilter_dir=/mnt/bogofilter
wordlist r,wordlist,wordlist.db,1
spam_header_name=X-Bogosity
spamicity_tags = Yes, No, Unsure
spamicity_formats = %0.6f, %0.6f, %0.6f
